{
    "title": "Scraper input",
    "description": "Input for the Instagram scraper",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "usernames": {
            "title": "Usernames to scrape",
            "type": "array",
            "description": "usernames to start with",
            "prefill": [
                { "username": "instagram" },
                { "username": "steelseries" }
            ],
            "editor": "stringList"
        },
        "proxy":{
            "title": "Proxy configuration",
            "type": "object",
            "description": "Select proxies to be used by your crawler.",
            "prefill": { "useApifyProxy": true },
            "editor": "proxy"
        }
    },
    "required": ["usernames", "proxy"]
}
