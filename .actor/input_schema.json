{
    "title": "Scraper input",
    "description": "Input for the Instagram scraper",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "usernames": {
            "title": "Usernames to scrape",
            "type": "array",
            "description": "usernames to start with",
            "prefill": [
                { "username": "instagram" },
                { "username": "steelseries" }
            ],
            "editor": "stringList"
        },
        "http_proxy": {
            "title": "HTTP_PROXY",
            "type": "string",
            "description": "HTTP Proxy to use",
            "prefill": "0.0.0.0",
            "editor": "keyValue"
        },
        "https_proxy": {
            "title": "HTTPS_PROXY",
            "type": "string",
            "description": "HTTPS Proxy to use",
            "prefill": "0.0.0.0",
            "editor": "keyValue"
        }
    },
    "required": ["usernames", "http_proxy", "https_proxy"]
}
